{
    "version": 2,
    "builds": [
      {
        "src": "client/dist/**/*",
        "use": "@vercel/static"
      },
      {
        "src": "api/**/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1.js"
      },
      {
        "src": "/(.*)",
        "dest": "/client/dist/$1"
      }
    ]
  }
  